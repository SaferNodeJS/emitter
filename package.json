{
  "name": "fbemitter",
  "version": "3.0.0-beta",
  "keywords": [
    "clientside"
  ],
  "bugs": "https://github.com/facebook/emitter/issues",
  "license": "BSD-3-Clause",
  "files": [
    "LICENSE",
    "PATENTS",
    "README.md",
    "index.js",
    "lib/"
  ],
  "main": "lib/EventEmitter.js",
  "repository": "facebook/emitter",
  "scripts": {
    "build": "gulp build",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test jest",
    "postinstall": "npm install --package-lock;npm audit fix"
  },
  "dependencies": {
    "audit": "0.0.6",
    "fbjs": "^0.8.4",
    "fix": "0.0.6",
    "uglifyjs-webpack-plugin": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-transform-flow-strip-types": "^7.6.3",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-react": "^7.6.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-module-map": "^1.0.1",
    "babel-preset-fbjs": "^3.3.0",
    "del": "^2.2.0",
    "eslint": "^6.5.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.16.0",
    "fbjs-scripts": "^0.8.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-flatten": "^0.2.0",
    "gulp-header": "^1.8.2",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.6",
    "jest": "^25.0.0",
    "jest-cli": "^25.0.0",
    "object-assign": "^4.0.1",
    "webpack-stream": "^5.2.1"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "modulePathIgnorePatterns": [
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "rootDir": "./",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/(?!(__forks__/fetch.js$|fetch/))"
    ],
    "transform": {
      ".*": "<rootDir>/scripts/jest/preprocessor.js"
    }
  }
}
